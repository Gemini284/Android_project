<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity_Organizacion">


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="99dp"
        android:layout_height="62dp"
        android:layout_marginBottom="128dp"
        app:layout_constraintBottom_toTopOf="@+id/bttnPuntos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/no" />

    <TextView
        android:id="@+id/Nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Banco de Alimentos"
        android:textAlignment="center"
        android:textColor="@color/red"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <Button
        android:id="@+id/editInven"
        android:layout_width="155dp"
        android:layout_height="0dp"
        android:layout_marginTop="20dp"
        android:autoSizeTextType="uniform"
        android:background="@drawable/btn_other"
        android:text="Editar Inventario"
        app:layout_constraintEnd_toEndOf="@+id/Dona"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="@+id/Dona"
        app:layout_constraintTop_toBottomOf="@+id/Dona" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="146dp"
        android:layout_height="62dp"
        app:layout_constraintBottom_toBottomOf="@+id/editInven"
        app:layout_constraintEnd_toStartOf="@+id/editInven"
        app:layout_constraintTop_toTopOf="@+id/editInven"
        app:srcCompat="@drawable/basket"
        tools:srcCompat="@drawable/basket" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="145dp"
        android:layout_height="63dp"
        app:layout_constraintBottom_toBottomOf="@+id/bttnPuntos"
        app:layout_constraintEnd_toStartOf="@+id/bttnPuntos"
        app:layout_constraintTop_toTopOf="@+id/bttnPuntos"
        app:srcCompat="@drawable/icono_puntos" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="144dp"
        android:layout_height="62dp"
        app:layout_constraintBottom_toBottomOf="@+id/bttnEvento"
        app:layout_constraintEnd_toStartOf="@+id/bttnEvento"
        app:layout_constraintTop_toTopOf="@+id/bttnEvento"
        app:srcCompat="@drawable/icono_eventos"
        tools:srcCompat="@drawable/icono_eventos" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="139dp"
        android:layout_height="62dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/Dona"
        app:layout_constraintEnd_toStartOf="@+id/Dona"
        app:layout_constraintTop_toTopOf="@+id/Dona"
        app:srcCompat="@drawable/coin_icon"
        tools:srcCompat="@drawable/coin_icon" />

    <Button
        android:id="@+id/bttnEvento"
        android:layout_width="155dp"
        android:layout_height="0dp"
        android:layout_marginTop="20dp"
        android:autoSizeTextType="uniform"
        android:background="@drawable/btn_other"
        android:text="Eventos"
        app:layout_constraintEnd_toEndOf="@+id/bttnPuntos"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/bttnPuntos" />

    <Button
        android:id="@+id/bttnPuntos"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:autoSizeTextType="uniform"
        android:background="@drawable/btn_other"
        android:text="Puntos de recolecciÃ³n"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.71"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.404" />

    <Button
        android:id="@+id/buttonLog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_bg"
        android:text="Log off"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.791"
        app:layout_constraintStart_toStartOf="@+id/editInven"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.932" />

    <TextView
        android:id="@+id/textView_empresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="organizacion@gmail.com"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/Nombre"
        app:layout_constraintStart_toStartOf="@+id/Nombre"
        app:layout_constraintTop_toBottomOf="@+id/Nombre" />

    <Button
        android:id="@+id/Dona"
        android:layout_width="155dp"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        android:autoSizeTextType="uniform"
        android:background="@drawable/btn_other"
        android:text="Donaciones"
        app:layout_constraintEnd_toEndOf="@+id/bttnEvento"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView10"
        app:layout_constraintTop_toBottomOf="@+id/bttnEvento" />

</androidx.constraintlayout.widget.ConstraintLayout>